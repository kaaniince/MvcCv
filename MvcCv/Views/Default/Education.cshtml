@using MvcCv.Models.Entity;
@model List<MvcCv.Models.Entity.TblEducation>


<div class="resume-section-content">
    <h2 class="mb-5">Education</h2>
    @foreach (var item in Model)
    {
        <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
            <div class="flex-grow-1">
                <h3 class="mb-0">@item.Header</h3>
                <div class="subheading mb-3">@item.SubTitle1</div>
                <div>@item.SubTitle2</div>
                <p>@item.GNO</p>
            </div>
            <div class="flex-shrink-0"><span class="text-primary">@item.Date</span></div>
        </div>
    }
</div>
