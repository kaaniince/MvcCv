
@{
    ViewBag.Title = "GetAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@model MvcCv.Models.Entity.TblLogin

<br />
<h4>Selected Admin</h4>
<br />

<form class="form-group" method="post">

    @Html.Label("Admin ID")
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    <br />
    @Html.Label("Nickname")
    @Html.TextBoxFor(x => x.Nickname, new { @class = "form-control" })
    <br />
    @Html.Label("Password")
    @Html.TextBoxFor(x => x.Password, new { @class = "form-control" })
    <br />

    <button class="btn btn-info">Update</button>
</form>




