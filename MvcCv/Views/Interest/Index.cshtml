@using MvcCv.Models.Entity;
@model List<Tbl_Interests>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Page Of Update Hobby</h2>
<br />
<form method="post">
    @foreach (var item in Model)
    {
        <label>Description 1</label>
        <textarea rows="5" class="form-control" name="Description1">@item.Description1</textarea>

        <label>Description 2</label>
        <textarea rows="5" class="form-control" name="Description2">@item.Description2</textarea>


    }
    <br />
    <button class="btn btn-success">Update</button>
</form>